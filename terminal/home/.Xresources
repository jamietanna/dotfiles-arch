XTerm*termName:xterm-256color
XTerm*faceName:Liberation Mono:style=Regular:size=12
! 115 = 92 * 1.25
! Xft.dpi: 115
Xft.dpi: 92

#include ".theme_colours.h"

! special
*.foreground:   				col_fg
*.background:   				col_bg
*.cursorColor:  				col_fg

! black
*.color0:	 					col_black
*.color8:	 					col_black_dark

! red
*.color1:						col_red
*.color9:						col_red_dark

! green
*.color2:   					col_green
*.color10:  					col_green_dark

! yellow
*.color3:  						col_yellow
*.color11: 						col_yellow_dark

! blue
*.color4: 						col_blue
*.color12:						col_blue_dark

! magenta
*.color5:  					 	col_magenta
*.color13: 					 	col_magenta_dark

! cyan
*.color6:  						col_cyan
*.color14: 						col_cyan_dark

! white
*.color7:						col_white
*.color15:						col_white_dark


rofi.font:       				LiberationMono 10
rofi.terminal:   				xterm
rofi.background: 				col_bg
rofi.foreground: 				col_fg
rofi.highlightbg: 				col_cyan
rofi.highlightfg: 				col_fg
rofi.bordercolor: 				col_white
rofi.color-enabled:				true

rofi.color-normal: 				col_cyan,col_bg,col_cyan_dark,col_blue_dark,col_bg
rofi.color-urgent: 				col_yellow_dark,col_white_true,col_yellow_dark,col_green_dark,col_white_true
rofi.color-active: 				col_red_dark,col_fg,col_red_dark,col_red,col_fg
rofi.color-window: 				col_cyan,col_white_true

#ifdef LOCAL_EXISTS
#include ".Xresources.local"
#endif
