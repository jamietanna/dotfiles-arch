XTerm*faceName:Liberation Mono:style=Regular:size=10
Xft.dpi: 96

#define col_bg 					#262626
#define col_fg 					#d0d0d0
#define col_black1 				#3a3a3a
#define col_black2 				#585858
#define col_red1 				#e82a41
#define col_red2 				#ff5faf
#define col_green1 				#00af5f
#define col_green2 				#afdf00
#define col_yellow1 			#df875f
#define col_yellow2 			#dfaf00
#define col_blue1 				#00afaf
#define col_blue2 				#00e0e0
#define col_magenta1 			#85678f
#define col_magenta2 			#af87af
#define col_cyan1 				#5e8d87
#define col_cyan2 				#8abeb7
#define col_white1 				#707880
#define col_white2 				#c5c8c6
#define col_white_true			#FFFFFF

#define col_black 				col_black1
#define col_red 				col_red1
#define col_green 				col_green1
#define col_yellow 				col_yellow1
#define col_blue 				col_blue1
#define col_magenta 			col_magenta1
#define col_cyan 				col_cyan1
#define col_white 				col_white1

#define col_black_dark 			col_black2
#define col_red_dark 			col_red2
#define col_green_dark 			col_green2
#define col_yellow_dark 		col_yellow2
#define col_blue_dark 			col_blue2
#define col_magenta_dark 		col_magenta2
#define col_cyan_dark 			col_cyan2
#define col_white_dark 			col_white2


! Based off NLKNguyen/papercolor-theme

! special
*.foreground:   				col_fg
*.background:   				col_bg
*.cursorColor:  				col_fg

! black
*.color0:	 					col_black
*.color8:	 					col_black_dark

! red
*.color1:						col_red
*.color9:						col_red_dark

! green
*.color2:   					col_green
*.color10:  					col_green_dark

! yellow
*.color3:  						col_yellow
*.color11: 						col_yellow_dark

! blue
*.color4: 						col_blue
*.color12:						col_blue_dark

! magenta
*.color5:  					 	col_magenta
*.color13: 					 	col_magenta_dark

! cyan
*.color6:  						col_cyan
*.color14: 						col_cyan_dark

! white
*.color7:						col_white
*.color15:						col_white_dark


rofi.font:       				LiberationMono 10
rofi.terminal:   				xterm
rofi.key-run:    				mod4-d
rofi.key-ssh:    				mod4-shift-Return
rofi.key-window: 				mod4-w
rofi.background: 				col_bg
rofi.foreground: 				col_fg
rofi.highlightbg: 				col_cyan
rofi.highlightfg: 				col_fg
rofi.bordercolor: 				col_white
rofi.color-enabled:				true

rofi.color-normal: 				col_cyan,col_bg,col_cyan_dark,col_blue_dark,col_bg
rofi.color-urgent: 				col_yellow_dark,col_white_true,col_yellow_dark,col_green_dark,col_white_true
rofi.color-active: 				col_red_dark,col_fg,col_red_dark,col_red,col_fg
! color-window - window-background, border
! (NOTE: second one won't be parsed if it's a variable)
rofi.color-window: 				col_cyan,#FFFFFF
